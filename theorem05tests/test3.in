LIB "resolve.lib";

ring A = (0,complex),(x(1..3), y(1..3), w),lp;

poly f = x(1)^2 + x(2)^2 - x(3)^2;// max length 1

poly g = y(1)*y(2)*y(3); //max length 2, first center is origin

ideal J = w^3 + f*w + g;

ideal C = Center(J);

printf("f: %s, max length: %s", f, 2);
printf("g: %s, max length: %s", g, 2);

printf("result polynomial: %s", J);
printf("first center of blowup: %s", Center(J));

list l = resolve(J);
presentTree(J);

quit;
